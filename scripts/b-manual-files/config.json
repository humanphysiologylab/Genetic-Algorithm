        {"name": "g_b_Na_scaler", "init": 1, "bounds": [0.1, 10], "gamma": 0.015, "scale": "log"},
        {"name": "PNaK_scaler", "init": 1, "bounds": [0.1, 10], "gamma": 0.015, "scale": "log"},
        {"name": "g_b_Ca_scaler","init": 1, "bounds": [0.1, 10], "gamma": 0.015, "scale": "log"},
        {"name": "g_PCa_scaler", "init": 1,"bounds": [0.1, 10], "gamma": 0.015, "scale": "log"},
