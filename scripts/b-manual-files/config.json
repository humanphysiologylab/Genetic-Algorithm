    "baselines": [
        {
		"name": "1000",
            "filename_phenotype": "./3/20200903 - Atrial EHM - 3 Hz pacing - rec18 - 1 Hz.txt",
            "filename_state": "./states-Default/state_1000.txt",
            "params": [
                {"name": "stim_period", "value": 1000},
