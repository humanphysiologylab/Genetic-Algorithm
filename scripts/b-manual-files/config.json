{
    "comment": "this config file is for spontan. immature Kernik-Clancy model",
    "script": "Genetic Algorithm",
    "max_time": 10000,
    "start_record_time": 0,
    "dump_period": 1,
    "dump_filename": "immature_KC.csv",
    "dump_vars": ["V", "Nai", "Ki", "i_K1", "i_Na", "i_f", "i_to", "i_Kr", "i_Ks", "i_NaK", "time"],
    "mode_comment": "test mode generates default baselines and OVERWRITES corresponding files",
    "mode_comment2": "remove mode field or change test to anything else to run optimization only",
    "mode": "notest",
    "test_beats": 20000,
    "t_sampling": 1,
    "n_organisms": 2000,
    "n_elites": 200,
    "n_generations": 400,
    "n_beats": 9,
    "eta_crossover": 10,
    "crossrate": 0.5,
    "gamma": 1,
    "eta_mutation": 20,
    "mutrate": 1,
    "mutation_type": "Cauchy",
    "is_AP_normalized": 0,
    "RESET_STATES": 0,
    "regularization_alpha": 0,
    "global": [
        {"name": "stim_flag", "value": 2},
        {"name": "i_stim_Amplitude", "value": 50},
        {"name": "i_stim_PulseDuration", "value": 3},
        {"name": "K_fixed", "value": 0},
        {"name": "Na_fixed", "value": 0},
        {"name": "g_kur_scaler", "init": 1, "bounds": [0.1, 10], "gamma": 0.015, "scale": "log"},
        {"name": "g_f_scaler", "init": 1, "bounds": [0.1, 10], "gamma": 0.015, "scale": "log"},
        {"name": "g_Na_scaler", "init": 1, "bounds": [0.1, 10], "gamma": 0.015, "scale": "log"},
