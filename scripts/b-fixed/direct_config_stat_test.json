{
    "comment": "this config file is for spontan. immature Kernik-Clancy model",
    "script": "Direct Problem",
    "max_time": 10000000,
    "start_record_time": 0,
    "dump_period": 100,
    "dump_filename": "stat",
    "dump_vars": [
        "V",
        "Nai",
        "Ki",
	"Ca_SR",
	"Cai",
        "time"
    ],
    "mode_comment": "test mode generates default baselines and OVERWRITES corresponding files",
