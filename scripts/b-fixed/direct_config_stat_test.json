    "test_beats": 20000,
    "t_sampling": 1,
    "n_organisms": 8000,
    "n_elites": 800,
    "n_generations": 700,
    "n_beats": 20,
    "eta_crossover": 10,
    "crossrate": 0.5,
    "gamma": 1,
    "eta_mutation": 20,
    "mutrate": 1,
    "mutation_type": "Cauchy",
    "is_AP_normalized": 0,
    "RESET_STATES": 1,
    "regularization_alpha": 0.1,
    "global": [
        {
            "name": "stim_flag",
            "value": 2
        },
        {
            "name": "i_stim_Amplitude",
